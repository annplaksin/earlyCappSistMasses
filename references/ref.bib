@article{Dean.1984,
 abstract = {This is a study of the manuscripts of sacred polyphony prepared at the scriptorium of the Sistine Chapel in the early sixteenth century. These are the MSS Cappella Sistina 16, 23, 26, 41, 42, 44, 45, 46, 49, 55, and 63, and Capella Giulia XII.2 (all in the Vatican Library), and the Medici Codex (now in the Biblioteca Laurenziana in Florence). These manuscripts (except for the Medici Codex) are composites of independent $\backslash$dq containing one or a very few pieces, later assembled into volumes. The manuscripts therefore are not of unitary date, and the alternation of scribes within the manuscripts is extremely complex. In the first chapter I present the archival evidence for the personnel of the Sistine scriptorium, identifying three scribes as likely to have contributed to the manuscripts under investigation. I then discuss manuscript structure and the copying process, and conclude the chapter with an exposition of my method of scribal analysis: how I classify and distinguish the hands of the several scribes (for they never sign their work), and how I may determine a chronology of their activity. Chapters 2-5 are devoted in turn to the scribes in chronological order: Johannes Orceau (active 1501-1512), Claudius Gellandi (ca. 1511-ca. 1521), four anonymous scribes associated with Gellandi (ca. 1518-ca. 1520), and Claudius Bouchet (ca. 1521-1527). Each copyist poses different problems of identification and chronology, requiring further development of methodology. These chapters are copiously illustrated with photographs and exhaustive charts of the alphabetic and musical characters used by each scribe. In Chapter 6 I investigate the foreign hands (correctors and annotators) in the manuscripts. This gives rise to digressions on text underlay and the history of the musical repertory of the Sistine choir. Chapter 7 is a detailed presentation of the conclusions of my study: a piece-by-piece catalogue of each manuscript. In Chapter 8 I present, as a guide to future research, a discussion of the value of scribal analysis to problems of textual criticism. Many individual pieces of music are discussed in the course of the dissertation.},
 author = {Dean, Jeffrey J.},
 year = {1984},
 title = {The Scribes of the Sistine Chapel, 1501-1527},
 url = {http://search.proquest.com/docview/303339208},
 keywords = {papal chapel},
 journal = {ProQuest Dissertations and Theses}
}


