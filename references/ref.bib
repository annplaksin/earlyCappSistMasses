@article{Brauner.1982,
 abstract = {Johannes Parvus was the principal scribe of polyphony for the Papal chapel (Cappella Sistina) from ca. 1535 to 1580. His work included all or part of twenty-six choirbooks made for the Cappella Sistina, the Cappella Giulia (the chapel at St. Peter's), S. Maria Maggiore in Rome, and the cathedral seminary in Lucca. The books contain roughly fifty Masses, one hundred and thirty motets, two sets of Vespers Hymns, three cycles of Magnificats and other individual liturgical works: one of the largest, most significant repertories of the sixteenth century. The composers represented in the repertory are primarily those associated with the Papal chapel--Costanzo Festa, Morales, Arcadelt and Palestrina--or those of wide international reputation, such as Josquin, Mouton and Jacquet of Mantua.

Parvus dated relatively few of his copies; his books, moreover, are constructed in such a way as to allow a single volume to contain material from widely differing periods. A reliable chronology, however, can be achieved by correlating various sorts of external evidence--dated entries, archival data, heraldry, paper, format and page layout--with the readily perceptible changes in Parvus's script over his long career. In this manner, it has been possible to assign any page in his output to a period of no more than five to ten years. In addition, a study of the readings of the works Parvus copied establishes that the scribe had access to varied exemplars for his repertory, sometimes copying a single piece from more than one source on different occasions. In some instances the exemplars can be positively identified.

On the basis of the chronology, it is possible to chart the transformation of the repertory of the Papal chapel as it changed from a major Renaissance musical institution at the beginning of Parvus's career to a bastion of conservatism and classicism in the early 1560s, and finally to a revitalized center of post-Tridentine reform. The developments in the repertory coincide with the changes in attitudes of the Church and the Papacy toward music during the entire Counter-Reformation period.},
 author = {Brauner, Mitchell Paul},
 year = {1982},
 title = {The Parvus Manuscripts: A Study of Vatican Polyphony, ca. 1535 to 1580},
 url = {http://search.proquest.com/docview/303189507},
 keywords = {papal chapel},
 journal = {ProQuest Dissertations and Theses  {\%} This file was created with Citavi 5.7.1.0}
}


